{
  "enabled": true,
  "name": "Auto Git Push",
  "description": "Automatically stages, commits, and pushes all changes to GitHub when the agent finishes working",
  "version": "1",
  "when": {
    "type": "agentStop"
  },
  "then": {
    "type": "runCommand",
    "command": "git add -A && git diff --cached --quiet || git commit -m \"chore: auto-commit\" && git push origin main"
  }
}
